<div id="container" class="containerProduto">
    <div class="container1Produto">
        <div>
            <button class="btnProduto">
                Novo
            </button>
        </div>
    </div>
    <div class="container2Produto">
        <table class="TableContainerProduto">
            <thead class="theadProduto">
            <tha>
                Código
            </tha>
            <tha>
                Descrição
            </tha>
            <tha>
                Opções
            </tha>
            </thead>
            <tbody>
           
                @foreach (var item in ViewBag.Produtos)
                {
                    <tr>
                        <td class="tdProduto">@item.codigo</td>
                        <td class="tdProduto">@item.descricao</td>
                        <td class="tdProduto">
                            <div class="IconesContainerProduto">
                                <img class="iconelixo" src="imagens/trash-solid.svg"/>
                                <div class="Icone2Produto">
                                </div>
                            </div>
                        </td>
                    </tr>
               
                }
            </tbody>

        
        </table>
    </div>
    <div class="container3Produto">
        <div>
            <button class="btnProduto BtnAzulColoProduto">
                Voltar
            </button>
        </div>
    </div>
</div>

